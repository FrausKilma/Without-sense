program WithoutSense;

uses
  Vcl.Forms,
  UHighVoltage in '..\High voltage\UHighVoltage.pas' {MainForm},
  UHostName in '..\High voltage\UHostName.pas' {FrmHostName},
  UDebug in '..\High voltage\UDebug.pas' {FrmDebug},
  UArdEmul in '..\High voltage\UArdEmul.pas' {FrmEmul},
  UThreadConnect in '..\High voltage\UThreadConnect.pas';

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.Title := 'Shadow of inquisition';
  Application.CreateForm(TMainForm, MainForm);
  Application.CreateForm(TFrmHostName, FrmHostName);
  Application.CreateForm(TFrmDebug, FrmDebug);
  Application.CreateForm(TFrmEmul, FrmEmul);
  Application.Run;
end.
